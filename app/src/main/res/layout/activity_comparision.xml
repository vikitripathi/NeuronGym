<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:materialdesign="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.edlogiq.neurongym.neurongym.Comparision">


    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:paddingBottom="20dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">


        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/linearLayout2"
            android:layout_weight="1"
            android:weightSum="1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Gender"
                android:layout_weight="0.30"
                android:textColor="@color/text_color"
                android:padding="5dp"
                android:gravity="center_horizontal"
                android:id="@+id/textView1" />

            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/linearLayout4"
                android:weightSum="3"
                android:layout_gravity="center_horizontal"
                android:layout_weight="0.70"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_width="match_parent"
                android:layout_height="70dp">


                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/linearLayout6"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <com.gc.materialdesign.views.ButtonFloatSmall
                        android:id="@+id/buttonFloatSmall1"
                        android:layout_gravity="center_horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/text_color"
                        materialdesign:iconDrawable="@drawable/gender_all"/>

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:id="@+id/linearLayout7"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">


                    <com.gc.materialdesign.views.ButtonFloatSmall
                        android:id="@+id/buttonFloatSmall2"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/text_color"
                        materialdesign:iconDrawable="@drawable/gender_male"/>

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:id="@+id/linearLayout8"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">


                    <com.gc.materialdesign.views.ButtonFloatSmall
                        android:id="@+id/buttonFloatSmall3"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/text_color"
                        materialdesign:iconDrawable="@drawable/gender_female"/>

                </LinearLayout>



            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/linearLayout3"
            android:layout_weight="1"
            android:weightSum="1"

            android:layout_width="wrap_content"
            android:layout_height="match_parent">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Location"
                android:layout_weight="0.30"
                android:textColor="@color/text_color"
                android:padding="5dp"
                android:gravity="center_horizontal"
                android:id="@+id/textView2" />

            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/linearLayout5"
                android:weightSum="2"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_weight="0.70"
                android:layout_marginTop="0dp"
                android:layout_width="match_parent"
                android:layout_height="70dp">


                <LinearLayout
                    android:orientation="vertical"
                    android:id="@+id/linearLayout9"
                    android:layout_weight="1"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginLeft="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <com.gc.materialdesign.views.ButtonFloatSmall
                        android:id="@+id/buttonFloatSmall4"
                        android:layout_gravity="center_horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/text_color"
                        materialdesign:iconDrawable="@drawable/location_global"/>

                </LinearLayout>



                <LinearLayout
                    android:orientation="vertical"
                    android:id="@+id/linearLayout10"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <com.gc.materialdesign.views.ButtonFloatSmall
                        android:id="@+id/buttonFloatSmall5"
                        android:layout_gravity="center_horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/text_color"
                        materialdesign:iconDrawable="@drawable/location_country"/>



                </LinearLayout>


            </LinearLayout>


        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/linearLayout11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:paddingBottom="20dp"
        android:layout_below="@+id/linearLayout1"
        android:layout_centerHorizontal="true">


        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/linearLayout12"
            android:layout_weight="0.70"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Age"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:textColor="@color/text_color"
                android:id="@+id/textView3" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/linearLayout13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="6"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1.30">

            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/linearLayout14"
                android:layout_weight="1"
                android:layout_marginLeft="0dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">

                <com.gc.materialdesign.views.ButtonFloatSmall
                    android:id="@+id/buttonFloatSmall6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/text_color"
                    materialdesign:iconDrawable="@drawable/m"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="All"
                    android:gravity="center"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="2dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:textColor="@color/text_color"

                    android:id="@+id/textView4"
                    android:layout_below="@+id/buttonFloatSmall6"/>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/linearLayout15"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <com.gc.materialdesign.views.ButtonFloatSmall
                    android:id="@+id/buttonFloatSmall7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/text_color"
                    materialdesign:iconDrawable="@drawable/t" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Kid"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:gravity="center"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="2dp"
                    android:textColor="@color/text_color"
                    android:id="@+id/textView5"
                    android:layout_below="@+id/buttonFloatSmall7" />



            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/linearLayout16"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <com.gc.materialdesign.views.ButtonFloatSmall
                    android:id="@+id/buttonFloatSmall8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/text_color"
                    materialdesign:iconDrawable="@drawable/w" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Youth"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:paddingLeft="2dp"
                    android:gravity="center"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="2dp"
                    android:textColor="@color/text_color"
                    android:id="@+id/textView6"
                    android:layout_below="@+id/buttonFloatSmall8"/>



            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/linearLayout17"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1">


                <com.gc.materialdesign.views.ButtonFloatSmall
                    android:id="@+id/buttonFloatSmall9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/text_color"
                    materialdesign:iconDrawable="@drawable/t" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Adult"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:gravity="center"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="2dp"
                    android:textColor="@color/text_color"
                    android:id="@+id/textView7"
                    android:layout_below="@+id/buttonFloatSmall9" />


            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/linearLayout18"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1">


                <com.gc.materialdesign.views.ButtonFloatSmall
                    android:id="@+id/buttonFloatSmall10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/text_color"
                    materialdesign:iconDrawable="@drawable/f"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Mid Age"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:gravity="center"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="2dp"
                    android:textColor="@color/text_color"
                    android:id="@+id/textView8"
                    android:layout_below="@+id/buttonFloatSmall10" />



            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/linearLayout19"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <com.gc.materialdesign.views.ButtonFloatSmall
                    android:id="@+id/buttonFloatSmall11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/text_color"
                    materialdesign:iconDrawable="@drawable/s" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Elder"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:textSize="15sp"
                    android:gravity="center"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="2dp"
                    android:textColor="@color/text_color"
                    android:id="@+id/textView9"
                    android:layout_below="@+id/buttonFloatSmall11"/>


            </LinearLayout>





        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/linearLayout20"
        android:layout_width="fill_parent"
        android:layout_height="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_below="@+id/linearLayout11"
        android:layout_centerHorizontal="true">

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_alignParentTop="true"
            android:background="#bdbdbd"/>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/linearLayout21"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout20"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Your Standings"
            android:textColor="#ffffff"
            android:background="@drawable/layout_border2"
            android:gravity="center"
            android:padding="5dp"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="50dp"
            android:layout_gravity="center_horizontal"
            android:id="@+id/textView10" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/linearLayout22"
        android:layout_margin="20dp"
        android:weightSum="6"
        android:background="@color/text_color"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/linearLayout21"
        android:layout_centerHorizontal="true">






    </LinearLayout>

</RelativeLayout>
